<h1>Listing ExecScripts</h1>

<table>
  <thead>
    <tr>
      <th>Name Script</th>
      <th>Rollbacks</th>
      <th>Limit_errors</th>
      <th>Success</th>
      <th></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exec_scripts.each do |exec_script| %>
      <tr>
        <td><%= exec_script.script_name %></td>
        <td><%= exec_script.rollbacks %></td>
        <td><%= exec_script.limit_errors %></td>
        <td><%= exec_script.success %></td>
        <td><%= link_to "Show", exec_script %></td>
        <td><%= link_to "Edit", edit_exec_script_path(exec_script) %></td>
        <td><%= link_to "Destroy",exec_script, method: :delete, data: { confirm: "Are you sure?" } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to "New exec script", new_exec_script_path %>
